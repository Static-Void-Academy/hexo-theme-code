<nav id="navbar-override" class="navbar is-spaced" role="navigation" aria-label="main navigation">
  <div class="navbar-brand">
    <a class="navbar-item" href="<%- url_for('/') %>">
    </a>
    <a role="button" class="navbar-burger burger" aria-label="menu" aria-expanded="false" data-target="navbarItems" for="nav-toggle-state">
      <span aria-hidden="true"></span>
      <span aria-hidden="true"></span>
      <span aria-hidden="true"></span>
    </a>
  </div>

  <div id="navbarItems" class="navbar-menu is-size-5">
    <div class="navbar-start">
      <% Object.keys(theme.menu).forEach(function(key) { %>
        <% if (typeof(theme.menu[key]) == 'object') { %>
        <div class="navbar-item has-dropdown is-hoverable has-text-centered">
          <span class="navbar-item">
            <%= key %>
            <i class="arrow down"></i>
          </span>
          <div class="navbar-dropdown">
          <% Object.keys(theme.menu[key]).forEach(function(subkey) { %>
            <% if (theme.menu[key][subkey] == 'disabled') { %>
            <a class="navbar-item has-text-centered is-disabled"><%= subkey %></a>
            <% } else { %>
            <a class="navbar-item has-text-centered" href="<%- url_for(theme.menu[key][subkey]) %>"><%= subkey %></a>
            <% } %>
          <% }); %>
          </div>
        </div>
        <% } else { %>
        <a class="navbar-item has-text-centered" href="<%- url_for(theme.menu[key]) %>"><%= key %></a>
        <% } %>
      <% }); %>
    </div>
    <div class="navbar-end is-hidden-touch">
      <a class="navbar-item is-size-4" href="<%- theme.rss %>" style="margin-right: 10px;">
        <%- fa_inline('rss', {prefix: 'fas'}) %>
      </a>
      <%- partial('_partial/search') %>
    </div>
  </div>
</nav>
